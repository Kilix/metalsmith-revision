{
  "name": "metalsmith-revision",
  "version": "0.0.0-semantic-dummy",
  "description": "Compile only the changed files",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wcastand/metalsmith-revision.git"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "babel --copy-files src/ -d lib/",
    "watch": "babel -w src/ -d lib/",
    "commit": "git-cz",
    "test": "mocha -w --compilers js:babel-register",
    "test:single": "mocha --compilers js:babel-register",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "William Castandet <wcastand@gmail.com> (https://github.com/wcastand)",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "commitizen": "^2.8.4",
    "cz-conventional-changelog": "^1.1.6",
    "mocha": "^3.0.1",
    "rimraf": "^2.5.4",
    "semantic-release": "^4.3.5"
  },
  "dependencies": {
    "crypto": "0.0.3"
  }
}
